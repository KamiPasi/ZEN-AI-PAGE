{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "docs/.vitepress/dist",
  "installCommand": "npm install",
  "framework": "vitepress",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
